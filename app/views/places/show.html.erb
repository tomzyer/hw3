<h1><%= @place.name %></h1>


<ul>
  <% @entries.each do |entry| %>
    <ul>
      <h2><%= entry.title %></h2>
       <p>(<%= entry.posted_on %>)</p>
      <p><%= entry.description %></p>
      
    </ul>
  <% end %>
</ul>

<%= link_to 'New Post', new_place_entry_path(@place) %>

<%= link_to 'All Places', places_path %>
